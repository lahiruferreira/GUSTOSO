<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.administrator.gustoso.admin_dashboard">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/admin_txt_welcome"
            android:layout_width="378dp"
            android:layout_height="30dp"
            android:layout_centerHorizontal="true"
            android:layout_marginLeft="85dp"
            android:layout_marginTop="9dp"
            android:text="@string/admin_txt_welcome"
            android:textAllCaps="true"
            android:textColor="@android:color/background_dark"
            android:textSize="24sp"
            android:textStyle="bold|italic" />

    <ImageView
        android:id="@+id/admin_img2"
        android:layout_width="110dp"
        android:layout_height="90dp"
        android:layout_marginTop="45dp"
        android:layout_marginLeft="210dp"
        android:background="@drawable/deliveryview" />

        <Button
            android:id="@+id/admin_btn2"
            android:layout_width="110dp"
            android:layout_height="25dp"
            android:layout_marginLeft="210dp"
            android:layout_marginTop="140dp"
            android:background="#FFEA00"
            android:text="@string/admin_btn2"
            android:textColor="@android:color/black"
            android:textStyle="bold" />

    <ImageView
        android:id="@+id/admin_img4"
        android:layout_width="110dp"
        android:layout_height="100dp"
        android:layout_marginTop="200dp"
        android:layout_marginLeft="215dp"
        android:background= "@drawable/food" />

        <Button
            android:id="@+id/admin_btn4"
            android:layout_width="110dp"
            android:layout_height="25dp"
            android:layout_marginLeft="210dp"
            android:layout_marginTop="310dp"
            android:background="#FFEA00"
            android:text="@string/admin_btn4"
            android:textColor="@android:color/black"
            android:textStyle="bold" />

    <ImageView
        android:id="@+id/admin_img1"
        android:layout_width="110dp"
        android:layout_height="90dp"
        android:layout_marginLeft="24dp"
        android:layout_marginTop="45dp"
        android:background= "@drawable/delivery" />

        <Button
            android:id="@+id/admin_btn1"
            android:layout_width="100dp"
            android:layout_height="25dp"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="140dp"
            android:background="#FFEA00"
            android:text="@string/admin_btn1"
            android:textColor="@android:color/black"
            android:textStyle="bold" />

    <ImageView
        android:id="@+id/admin_img3"
        android:layout_width="110dp"
        android:layout_height="90dp"
        android:layout_marginTop="205dp"
        android:layout_marginLeft="25dp"
        android:background= "@drawable/empone" />

        <Button
            android:id="@+id/admin_btn3"
            android:layout_width="110dp"
            android:layout_height="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="310dp"
            android:background="#FFEA00"
            android:text="@string/admin_btn3"
            android:textColor="@android:color/black"
            android:textStyle="bold" />

    <ImageView
        android:id="@+id/admin_img5"
        android:layout_width="100dp"
        android:layout_height="90dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="370dp"
        android:background="@drawable/emptwo" />

        <Button
            android:id="@+id/admin_btn5"
            android:layout_width="110dp"
            android:layout_height="25dp"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="470dp"
            android:background="#FFEA00"
            android:text="@string/admin_btn5"
            android:textColor="@android:color/black"
            android:textStyle="bold" />

    <ImageView
        android:id="@+id/admin_img6"
        android:layout_width="110dp"
        android:layout_height="90dp"
        android:layout_marginTop="375dp"
        android:layout_marginLeft="210dp"
        android:layout_marginRight="24dp"
        android:background= "@drawable/empthree" />

        <Button
            android:id="@+id/admin_btn6"
            android:layout_width="120dp"
            android:layout_height="25dp"
            android:layout_marginLeft="205dp"
            android:layout_marginTop="470dp"
            android:background="#FFEA00"
            android:text="@string/admin_btn6"
            android:textColor="@android:color/black"
            android:textStyle="bold" />

    </RelativeLayout>

</ScrollView>